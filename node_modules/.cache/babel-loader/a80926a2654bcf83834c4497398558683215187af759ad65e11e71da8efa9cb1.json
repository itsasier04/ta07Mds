{"ast":null,"code":"import electricidadData from \"@/data/electricidad.json\";\nexport default {\n  data() {\n    return {\n      electricidad: electricidadData.electricidad,\n      incremento: 0,\n      decremento: 0,\n      nuevoIncremento: \"\",\n      nuevoDecremento: \"\",\n      placas: electricidadData.electricidad.numero_placas_solares\n    };\n  },\n  methods: {\n    incrementarConsumo() {\n      const valor = parseInt(this.nuevoIncremento);\n      if (!isNaN(valor) && valor > 0 && valor < 9999) {\n        this.incremento = valor;\n      } else {\n        alert(\"Por favor, introduce un número válido entre 1 y 9998.\");\n      }\n    },\n    decrementarConsumo() {\n      const valor = parseInt(this.nuevoDecremento);\n      if (!isNaN(valor) && valor > 0 && valor < 9999) {\n        this.decremento = valor;\n      } else {\n        alert(\"Por favor, introduce un número válido entre 1 y 9998.\");\n      }\n    },\n    incrementarPlacas() {\n      if (this.placas < 200) {\n        this.placas++;\n      } else {\n        alert(\"No puedes añadir más placas, has alcanzado el límite de 200.\");\n      }\n    },\n    calcularAutoconsumoDiario() {\n      return (this.placas / 136 * this.electricidad.autoconsumo_diario_kwh).toFixed(2);\n    },\n    calcularPorcentajeAutoconsumo() {\n      const consumoTotal = this.electricidad.consumo_anual_kwh + this.incremento * 220 - this.decremento * 220;\n      const autoconsumoTotal = this.calcularAutoconsumoDiario() * 365;\n      return (autoconsumoTotal / consumoTotal * 100).toFixed(2);\n    }\n  }\n};","map":{"version":3,"names":["electricidadData","data","electricidad","incremento","decremento","nuevoIncremento","nuevoDecremento","placas","numero_placas_solares","methods","incrementarConsumo","valor","parseInt","isNaN","alert","decrementarConsumo","incrementarPlacas","calcularAutoconsumoDiario","autoconsumo_diario_kwh","toFixed","calcularPorcentajeAutoconsumo","consumoTotal","consumo_anual_kwh","autoconsumoTotal"],"sources":["/home/asier/calculadora-sostenible/src/views/CalculadoraElectricitatView.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n      <main class=\"content\">\n        <h1>Consumo de Electricidad</h1>\n        <div class=\"data-card\">\n          <h2>Datos Generales</h2>\n          <p><strong>Número de Placas Solares:</strong> {{ placas }}</p>\n          <p><strong>Consumo Diario (Laboral):</strong> {{ electricidad.consumo_laboral_diario_kwh + incremento - decremento }} kWh</p>\n          <p><strong>Consumo Diario (Fin de Semana):</strong> {{ electricidad.consumo_fin_de_semana_diario_kwh }} kWh</p>\n          <p><strong>Consumo Mensual:</strong> {{ (electricidad.consumo_mensual_kwh + (incremento * 22) - (decremento * 22)) }} kWh</p>\n          <p><strong>Consumo Anual:</strong> {{ (electricidad.consumo_anual_kwh + (incremento * 220) - (decremento * 220)) }} kWh</p>\n          <p><strong>Autoconsumo Diario:</strong> {{ calcularAutoconsumoDiario() }} kWh</p>\n          <p><strong>Porcentaje de Autoconsumo:</strong> {{ calcularPorcentajeAutoconsumo() }}%</p>\n        </div>\n        \n        <div class=\"controls\">\n          <input v-model.number=\"nuevoIncremento\" type=\"number\" placeholder=\"Introduce kWh\" />\n          <button @click=\"incrementarConsumo\">Incrementar Consumo</button>\n        </div>\n        \n        <div class=\"controls\">\n          <input v-model.number=\"nuevoDecremento\" type=\"number\" placeholder=\"Introduce kWh\" />\n          <button @click=\"decrementarConsumo\">Decrementar Consumo</button>\n        </div>\n  \n        <div class=\"controls\">\n          <button @click=\"incrementarPlacas\">Añadir Placa Solar</button>\n        </div>\n      </main>\n    </div>\n  </template>\n  \n  <script>\n  import electricidadData from \"@/data/electricidad.json\";\n  \n  export default {\n    data() {\n      return {\n        electricidad: electricidadData.electricidad,\n        incremento: 0,\n        decremento: 0,\n        nuevoIncremento: \"\",\n        nuevoDecremento: \"\",\n        placas: electricidadData.electricidad.numero_placas_solares\n      };\n    },\n    methods: {\n      incrementarConsumo() {\n        const valor = parseInt(this.nuevoIncremento);\n        if (!isNaN(valor) && valor > 0 && valor < 9999) {\n          this.incremento = valor;\n        } else {\n          alert(\"Por favor, introduce un número válido entre 1 y 9998.\");\n        }\n      },\n      decrementarConsumo() {\n        const valor = parseInt(this.nuevoDecremento);\n        if (!isNaN(valor) && valor > 0 && valor < 9999) {\n          this.decremento = valor;\n        } else {\n          alert(\"Por favor, introduce un número válido entre 1 y 9998.\");\n        }\n      },\n      incrementarPlacas() {\n        if (this.placas < 200) {\n          this.placas++;\n        } else {\n          alert(\"No puedes añadir más placas, has alcanzado el límite de 200.\");\n        }\n      },\n      calcularAutoconsumoDiario() {\n        return ((this.placas / 136) * this.electricidad.autoconsumo_diario_kwh).toFixed(2);\n      },\n      calcularPorcentajeAutoconsumo() {\n        const consumoTotal = this.electricidad.consumo_anual_kwh + (this.incremento * 220) - (this.decremento * 220);\n        const autoconsumoTotal = this.calcularAutoconsumoDiario() * 365;\n        return ((autoconsumoTotal / consumoTotal) * 100).toFixed(2);\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .container {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 20px;\n  }\n  \n  .content {\n    width: 80%;\n    max-width: 800px;\n    text-align: center;\n  }\n  \n  .data-card {\n    background: var(--light-gray);\n    padding: 20px;\n    border-radius: 10px;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n    margin-top: 20px;\n  }\n  \n  .controls {\n    margin-top: 20px;\n    display: flex;\n    gap: 10px;\n    align-items: center;\n  }\n  \n  input {\n    padding: 10px;\n    border: 1px solid var(--medium-gray);\n    border-radius: 5px;\n    width: 150px;\n  }\n  \n  button {\n    background-color: var(--primary-green);\n    color: var(--dark-gray);\n    padding: 10px 15px;\n    border: none;\n    border-radius: 5px;\n    cursor: pointer;\n    transition: background 0.3s;\n  }\n  \n  button:hover {\n    background-color: var(--secondary-green);\n  }\n  </style>\n  "],"mappings":"AAiCE,OAAOA,gBAAe,MAAO,0BAA0B;AAEvD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAEF,gBAAgB,CAACE,YAAY;MAC3CC,UAAU,EAAE,CAAC;MACbC,UAAU,EAAE,CAAC;MACbC,eAAe,EAAE,EAAE;MACnBC,eAAe,EAAE,EAAE;MACnBC,MAAM,EAAEP,gBAAgB,CAACE,YAAY,CAACM;IACxC,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAIC,QAAQ,CAAC,IAAI,CAACP,eAAe,CAAC;MAC5C,IAAI,CAACQ,KAAK,CAACF,KAAK,KAAKA,KAAI,GAAI,KAAKA,KAAI,GAAI,IAAI,EAAE;QAC9C,IAAI,CAACR,UAAS,GAAIQ,KAAK;MACzB,OAAO;QACLG,KAAK,CAAC,uDAAuD,CAAC;MAChE;IACF,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnB,MAAMJ,KAAI,GAAIC,QAAQ,CAAC,IAAI,CAACN,eAAe,CAAC;MAC5C,IAAI,CAACO,KAAK,CAACF,KAAK,KAAKA,KAAI,GAAI,KAAKA,KAAI,GAAI,IAAI,EAAE;QAC9C,IAAI,CAACP,UAAS,GAAIO,KAAK;MACzB,OAAO;QACLG,KAAK,CAAC,uDAAuD,CAAC;MAChE;IACF,CAAC;IACDE,iBAAiBA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACT,MAAK,GAAI,GAAG,EAAE;QACrB,IAAI,CAACA,MAAM,EAAE;MACf,OAAO;QACLO,KAAK,CAAC,8DAA8D,CAAC;MACvE;IACF,CAAC;IACDG,yBAAyBA,CAAA,EAAG;MAC1B,OAAO,CAAE,IAAI,CAACV,MAAK,GAAI,GAAG,GAAI,IAAI,CAACL,YAAY,CAACgB,sBAAsB,EAAEC,OAAO,CAAC,CAAC,CAAC;IACpF,CAAC;IACDC,6BAA6BA,CAAA,EAAG;MAC9B,MAAMC,YAAW,GAAI,IAAI,CAACnB,YAAY,CAACoB,iBAAgB,GAAK,IAAI,CAACnB,UAAS,GAAI,GAAG,GAAK,IAAI,CAACC,UAAS,GAAI,GAAI;MAC5G,MAAMmB,gBAAe,GAAI,IAAI,CAACN,yBAAyB,CAAC,IAAI,GAAG;MAC/D,OAAO,CAAEM,gBAAe,GAAIF,YAAY,GAAI,GAAG,EAAEF,OAAO,CAAC,CAAC,CAAC;IAC7D;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}